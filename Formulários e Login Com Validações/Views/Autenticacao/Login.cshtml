@model Formulários_e_Login_Com_Validações.Models.Usuario

<div class="limiter">
    <div class="container-login100" style="background-color: #d4f2d1">
        <div class="col-md-8" style="background-color: #ffffff">
            <div>
                <br /><br />
                @if (TempData["Msglogin"] != null)
                {
                    <div class="alert alert-danger" role="alert">
                        <h4>@TempData["Msglogin"]</h4>
                        <a class="close" data-dismiss="alert">
                            <span class="glyphicon glyphicon-remove"></span>
                        </a>
                    </div>
                }
                <br /><br /><Br />
                <span class="login100-form-title p-b-49">
                    Login
                </span>


                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="wrap-input100 m-b-23">
                        @Html.LabelFor(model => model.Email, new { @class = "label-input100" })
                        @Html.TextBoxFor(model => model.Email, new { @class = "input100", placeholder = "Digite o Email Cadastrado", autocomplete = "off", required = "required" })
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>
                    <div id="input" class="wrap-input100 m-b-23">
                        @Html.LabelFor(model => model.Password, new { @class = "label-input100" })
                        @Html.PasswordFor(model => model.Password, new { @class = "input100", placeholder = "Digite a Senha de Acesso", autocomplete = "off", required = "required", id="txtSenha", onKeyup="verificaForcaDaSenha()" })
                        <div id="lblmensagem"></div>
                        <img src="~/Content/img/icon/iconsenha.png" alt="">
                    </div>
                    <div class="text-right p-t-8 p-b-31">
                        <a href="#">
                            Esqueceu sua senha?
                        </a>
                    </div>

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button class="login100-form-btn">
                                LOGAR
                            </button>
                        </div>
                    </div>
                }

                <div class="txt1 text-center p-t-54 p-b-20">
                    <span>
                        Ou Inscreva-se Usando
                    </span>
                </div>

                <div class="flex-c-m">
                    <a href="#" class="login100-social-item bg1">
                        <i class="fa fa-facebook"></i>
                    </a>

                    <a href="#" class="login100-social-item bg2">
                        <i class="fa fa-twitter"></i>
                    </a>

                    <a href="#" class="login100-social-item bg3">
                        <i class="fa fa-google"></i>
                    </a>
                </div>

                <div class="flex-col-c p-t-155">
                    <span class="txt1 p-b-17">
                        Ou Inscreva-se Usando
                    </span>

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <a href="~/Autenticacao/Registrar" class="login100-form-btn">
                                Não tenho Cadastro
                            </a>
                        </div>
                    </div>
                    <br /><br />
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/js/Autenticacao/verificaForcaDaSenha.js"></script>
